 address = {Göttingen and Bern},
 volume = {20},
 publisher = {Hogrefe},
 isbn = {380171117X},
 series = {Fortschritte der Psychotherapie},
 file = {Goebel 2003 - Tinnitus und Hyperakusis:Attachments/Goebel 2003 - Tinnitus und Hyperakusis.pdf:application/pdf}
